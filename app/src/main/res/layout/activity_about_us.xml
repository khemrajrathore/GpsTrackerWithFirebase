<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.buckleup.gpstracker.AboutUs">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:background="@drawable/aboutus"
        android:layout_marginTop="49dp"
        android:id="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/helpemail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/email_khemrajrathore1997_gmail_com"
        android:textSize="20dp"
        android:textStyle="bold|italic"
        tools:ignore="SpUsage" />

    <TextView
        android:id="@+id/helpnumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/helpemail"
        android:layout_marginStart="17dp"
        android:layout_marginTop="41dp"
        android:text="@string/mobile_number_918421466794"
        android:textSize="20dp"
        android:textStyle="bold|italic"
        tools:ignore="SpUsage" />

</RelativeLayout>
